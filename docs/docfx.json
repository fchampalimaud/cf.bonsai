{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "src": "../",
        "files": [
          "workflows/**"
        ]
      },
      {
        "files":[
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "docfx-tools/template",
      "template"
    ],
    "globalMetadata": {
      "_appName": "CF.Bonsai",
      "_appTitle": "CF.Bonsai",
      "_appLogoPath": "images/cf-logo.svg",
      "_appFooter": "<span>&copy; 2024 Champalimaud Foundation and Contributors</span> -<span>Made with <a href=\"https://dotnet.github.io/docfx\">docfx</a></span>",
      "_enableSearch": true,
      "pdf": true
    },
    "markdownEngineName": "markdig",
    "markdownEngineProperties": {
      "markdigExtensions": [
        "attributes",
        "customcontainers"
      ]
    },
    "xref": [
      "https://horizongir.github.io/opencv.net/xrefmap.yml",
      "https://horizongir.github.io/ZedGraph/xrefmap.yml",
      "https://horizongir.github.io/opentk/xrefmap.yml",
      "https://horizongir.github.io/reactive/xrefmap.yml",
      "https://bonsai-rx.org/docs/xrefmap.yml",
      "https://harp-tech.org/xrefmap.yml"
    ]
  }
}